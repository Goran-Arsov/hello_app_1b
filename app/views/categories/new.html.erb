<div class="container">
    <h1><strong>Create a new category</strong></h1>
    <br/>
    <%= form_for @category do |c|%>
        <p>
            <%= c.label :name %> <br/>
            <%= c.text_field :name %> <br/>
        </p>
        <p>
            <%= c.submit "Create Category"%>
        </p>
    <% end %>
    <br/>
    <p><%= link_to "Go To All Categories", categories_path %> | <%= link_to "Go To All Posts", posts_path %></p>
</div>