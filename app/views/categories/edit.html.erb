<div class="container">
    <h1><strong>Edit category</strong></h1>
    <br/>
    <%= form_for @category do |c|%>
        <p>
            <%= c.label :name %> <br/>
            <%= c.text_field :name %> <br/>
        </p>
        <p>
            <%= c.submit "Edit Category"%>
        </p>
    <% end %>
    
</div>